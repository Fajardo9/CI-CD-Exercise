pipeline {
    agent {
        label "windows"
    }
environment{
firstStudent = "Octubre"
secondStudent = "Noviembre"
}
parameters{
text(name:"Teacher", defaultValue: "Luisa", description:"teacher")
}
    stages {
        stage('work firstStudent') {
            steps {
                echo '${params.Teacher} sent ${firstStudent} to count from 1 to 3: 1'
            }
        }
    }
}